<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid
                android:angle="90"
                android:color="@color/green_700" />
            <padding
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp" />
            <!--android:dashGap="1dp"
            android:dashWidth="4dp"-->
            <corners android:radius="15dp" />
        </shape>
    </item>
    <item android:bottom="0dp">
        <shape android:shape="oval">
            <solid
                android:color="@color/green_500" />
            <padding
                android:bottom="5dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp" />
            <!--android:dashGap="1dp"
            android:dashWidth="4dp"-->
            <corners android:radius="15dp"
                />
        </shape>
    </item>
</layer-list>